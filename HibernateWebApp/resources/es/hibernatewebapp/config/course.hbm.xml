<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="es.hibernatewebapp.entities.Course" table="my_highschool.COURSE">
		<id name="id" column="ID_COURSE">
			<generator class="incremental"></generator>
		</id>
		<property name="name" column="NAME"></property>
		<property name="description" column="DESCRIPTION"></property>
		<property name="startDate" column="START_DATE"></property>
		<property name="endDate" column="END_DATE"></property>
		
	</class>
</hibernate-mapping>